#include<iostream>
#include<map>
#include<algorithm>
using namespace std;

class print
{
public:
	void operator()(pair<int, int> p)
	{
		cout << p.first << " " << p.second << endl;
	}
};

class compare
{
public:
	bool operator()(int v1, int v2)
	{
		return v1 > v2;
	}
};

int main()
{
	//map<int, int, greater<int>> p;
	map<int, int, compare> p;
	for (int i = 0; i < 4; i++)
	{
		p.insert(make_pair(i, i + 5));
	}
	//for_each(p.begin(), p.end(), print());
	

	return 0;
}
