#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;

//面试题中如果出现查找相邻元素，记得用这个

int main()
{
	vector<int> v;
	for (int i = 0; i < 5; i++)
		v.push_back(i);
		v.push_back(4);
		if (adjacent_find(v.begin(), v.end()) == v.end())
			cout << "未有相邻元素\n";
		else
	cout << *adjacent_find(v.begin(), v.end());
	return 0;
}
